#include <bits/stdc++.h>
using namespace std;
bool isPrime(int n, int q){
    n+=q;
    if (n==2){
        return true;
    }
    for (int i = 2; i < ((int)sqrt(n))+1; i++){
        if (n%i==0){
            return false;
        }
    }
    return true;
}
int main(){
    int n, q;
    cin >> n >> q;
    vector<int> d(n);
    vector<int> quir(q, 0);
    for (int i = 0; i < n; i++){
        cin >> d[i];
    }
    for (int i = 0; i < q; i++){
        int temp;
        cin >> temp;
        int cnt = 0;
        for (int j = 0; j < n; j++){
            if (isPrime(d[j], temp)){
                cnt++;
            }
        }
        if (cnt<=n){
            quir[i] = 1;
        }
    }
    for (int i = 0; i < q; i++){
        if (quir[i]){
            cout << "YES\n";
        }
        else{
            cout << "NO\n";
        }
    }
    
}
